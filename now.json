{
    "version": 2,
    "public": true,
    "alias": ["testauth.net"],
    "builds": [
        { "src": "package.json", "use": "@now/static-build"}
    ],
    "routes": [
        { "src": "/$", "dest": "dist/index.html"},
        { "src": "/index.html", "dest": "index.html"},
        { "src": "/test.html", "dest": "./dist/test.html"},
        { "src": "/test", "dest": "test.html"},
        { "src": "/.*", "status": 403}
    ]
}