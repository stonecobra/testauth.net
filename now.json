{
    "version": 2,
    "public": true,
    "alias": ["testauth.net"],
    "routes": [
        { "src": "/", "dest": "index.html"},
        { "src": "/auth/login", "dest": "auth/login/index.js"},
        { "src": "/auth/logout", "dest": "auth/logout/index.js"},
        { "src": "/auth/status", "dest": "auth/status/index.js"},
        { "src": "/.*", "status": 404}
    ]
}